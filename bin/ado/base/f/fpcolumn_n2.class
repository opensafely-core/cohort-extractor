*! version 1.0.0  03may2019
version 16

class fpcolumn_n2 {

} , inherit(fpcolumn)

program .new
	syntax [ , Style(passthru) *]

	if `"`style'"' == "" {
		local style "style(scheme fpn2)"
	}

	.labels = `""Control" "N""'

	.Super.new , `macval(style)' `macval(options)'
end

program .fill_varidx
	.varidx[1] = `.serset.seriesof _meta_n2'
	.GetFormat format : _n2
	.formats[1] = `"`format'"'
end

exit
