*! version 1.0.0  03may2019
version 16

class fpcolumn_b {

} , inherit(fpcolumn)

program .new
	syntax [ , Style(passthru) *]

	if `"`style'"' == "" {
		local style "style(scheme fpb)"
	}

	.labels = `""Treatment" "No""'

	.Super.new , `macval(style)' `macval(options)'
end

program .fill_varidx
	.varidx[1] = `.serset.seriesof _meta_b'
	.GetFormat format : _b
	.formats[1] = `"`format'"'
end

exit
