version 16

class {

	plot_width	 = .gsize.new

 	coltitle	 = .textboxstyle.new
 	collabel	 = .textboxstyle.new

 	itemtextstyle	 = .textboxstyle.new
 	grptextstyle	 = .textboxstyle.new
 	overalltextstyle = .textboxstyle.new
 	qbtextstyle	 = .textboxstyle.new

 	itemlabelstyle	 = .textboxstyle.new
 	grplabelstyle	 = .textboxstyle.new
 	overalllabelstyle = .textboxstyle.new
 	qblabelstyle	 = .textboxstyle.new

	grpmarkerstyle	 = .seriesstyle.new
	allmarkerstyle	 = .seriesstyle.new

	study_gap	 = .gsize.new
	group_gap	 = .gsize.new
	overall_gap	 = .gsize.new

} , inherit(graphstyle)

program .setstyle
	syntax [, Style(passthru)]

	.Super.setstyle, `style'
end

program .set
	syntax [, Style(passthru)]

	if `"`style'"' != "" {
		.setstyle, `style'
	}
end

exit
