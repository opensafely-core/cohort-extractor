*! version 1.0.0  03may2019
version 16

class fpcolumn_a {

} , inherit(fpcolumn)

program .new
	syntax [ , Style(passthru) *]

	if `"`style'"' == "" {
		local style "style(scheme fpa)"
	}

	.labels = `""Treatment" "Yes""'

	.Super.new , `macval(style)' `macval(options)'
end

program .fill_varidx
	.varidx[1] = `.serset.seriesof _meta_a'
	.GetFormat format : _a
	.formats[1] = `"`format'"'
end

exit
