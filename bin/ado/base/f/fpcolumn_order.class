*! version 1.0.0  06may2019
version 16

class fpcolumn_order {

} , inherit(fpcolumn)

program .new
	syntax [ , Style(passthru) *]

	if `"`style'"' == "" {
		local style "style(scheme fporder)"
	}

	.labels = `""_ORDER_""'

	.Super.new , `macval(style)' `macval(options)'
end

program .fill_varidx
	.varidx[1] = `.serset.seriesof _meta_order'
	.GetFormat format : _order
	.formats[1] = `"`format'"'
end

exit
