*! version 1.0.0  12may2019
version 16

class fpcolumn_se {

} , inherit(fpcolumn)

program .new
	syntax [ , Style(passthru) *]

	if `"`style'"' == "" {
		local style "style(scheme fpse)"
	}

	.labels = `""SE""'

	.Super.new , `macval(style)' `macval(options)'
end

program .fill_varidx
	.varidx[1] = `.serset.seriesof _meta_se'
	.GetFormat format : _se
	.formats[1] = `"`format'"'
end

program .grpcontent
	args c_text COLON res_prse grpid

	local val1 `.`res_prse'[`grpid'].se'
	if missing(`val1') {
		c_local `c_text'
		exit
	}
	mata: st_local("result", sprintf(`"`.mask'"', `val1'))

	c_local `c_text' `"`result'"'
end

exit
