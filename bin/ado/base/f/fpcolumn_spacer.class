*! version 1.0.0  12may2019
version 16

class fpcolumn_spacer {

} , inherit(fpcolumn)

program .new
	syntax [ , Style(passthru) *]

	if `"`style'"' == "" {
		local style "style(scheme fpspacer)"
	}

	.labels = `""'

	.Super.new , `macval(style)' `macval(options)'
end

program .make_mask
	* do nothing
end

program .content
	args c_text COLON obs

	c_local `c_text' `"`.alt_text'"'
end

program .grpcontent
	args c_text COLON res_prse grpid

	c_local `c_text' `"`.alt_text'"'
end

program .fill_varidx
	* do nothing
end

exit
