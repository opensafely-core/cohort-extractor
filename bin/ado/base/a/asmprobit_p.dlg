/*
	predict (after asmprobit)

*!  VERSION 1.2.0  04sep2014

*/

VERSION 14.0

INCLUDE _std_medium  // ** set POSTINIT to match
INCLUDE header

HELP hlp1, view("help asmprobit postestimation")
RESET res1

SCRIPT PREINIT
BEGIN
        setscale_hadj resource ASMPROBIT_P_DLG_HSCALE
END

SCRIPT POSTINIT
BEGIN
	script _p_gentype_setMedium
END

DIALOG main, label(resource PREDICT_DLG_TITLE) tabtitle(resource LBL_TAB_MAIN)
BEGIN
  INCLUDE  _p_gentype

  GROUPBOX gb_opts		_lft	_ls	_iwd	_ht8,		///
		label(resource LBL_PRODUCE)
  RADIO    rb_pr		_indent	_ss	_inwd	., first	///
		option(pr)						///
		onclickon(script _p_gentype_single)			///
		label(resource LBL_PROBABILITY_ALTERNATIVE_CHOSEN)
  RADIO    rb_xb		@	_ss	@	.,		///
		option(xb)						///
		onclickon(script _p_gentype_single)			///
		label(resource LBL_LINEAR_PREDICTION)
  RADIO    rb_stdp		@	_ss	@	.,		///
		option(stdp)						///
		onclickon(script _p_gentype_single)			///
		label(resource LBL_STANDARD_ERROR_LINEAR_PREDICTION)
  RADIO    rb_score		@	_ss	@	., last		///
		option(score)						///
		onclickon(script _p_gentype_multiple)			///
		label(resource LBL_PARAMETER_LEVEL_SCORES)
  CHECKBOX ck_altwise	_lft	_xls	_cwd1	.,			///
	option(altwise) label(resource LBL_ALTERNATIVE_WISE_DELETION)
END

INCLUDE _p_gentype_sc
INCLUDE ifin
INCLUDE _type_list_fd

PROGRAM command
BEGIN
	put "predict "
	put " " /program _p_gentype_output
	put " " /program ifin_output
	beginoptions
		option radio(main rb_pr rb_xb rb_stdp rb_score)
		option main.ck_altwise
	endoptions
END
