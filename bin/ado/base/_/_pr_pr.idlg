/*
	INCLUDE for pr() option of predict in PROGRAM part of dlg

	VERSION 1.0.0  21dec2002
*/

	beginoptions
		if main.rb_pr {
			put "pr("
			if main.en_pr_a {
				put main.en_pr_a
			}
			if !main.en_pr_a {
				put "."
			}
			put ","
			if main.en_pr_b {
				put main.en_pr_b
			}
			if !main.en_pr_b {
				put "."
			}
			put ")"
		}
	endoptions

