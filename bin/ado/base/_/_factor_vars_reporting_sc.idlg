SCRIPT factor_vars_reporting_enable
BEGIN
	rpt.gb_fv.enable
	rpt.ck_noempty.enable
	rpt.gb_base_lev.enable
	rpt.rb_nobasel.enable
	rpt.rb_basel.enable
	rpt.rb_allbasel.enable
END
