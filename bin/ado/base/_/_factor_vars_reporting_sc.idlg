SCRIPT factor_vars_reporting_disable
BEGIN
	rpt.gb_fv.disable
	rpt.ck_noempty.disable
	rpt.gb_base_lev.disable
	rpt.rb_nobasel.disable
	rpt.rb_basel.disable
