program define _gmtr
	version 6, missing
/*
	Grab the variables before parsing "if" and "in".
*/
	gettoken type 0 : 0
	gettoken mtr 0 : 0
	gettoken eqs 0 : 0			/* "=" */

	gettoken paren 0 : 0, parse("(), ")	/* "(" */

	gettoken year 0 : 0, parse("(), ")
	gettoken inc  0 : 0, parse("(), ")
	if `"`inc'"' == "," {
		gettoken inc  0 : 0, parse("(), ")
	}
	gettoken paren 0 : 0, parse("(), ")	/* ")" */
	if `"`paren'"' != ")" {
		error 198
	}

	syntax [if] [in] [, BY(string)]
	if `"`by'"' != "" {
		_egennoby mtr() `"`by'"'
		/* NOTREACHED */
	}


	quietly {
		tempvar touse
		mark `touse' `if' `in'

			/* Is year a variable or a constant? */
		cap conf var `year'
		if !_rc { local yname `year' }

			/* Is inc a variable or a constant? */
		cap conf var `inc'
		if !_rc {
			local inctype : type `inc'
		}
		else	local inctype double

			/* make income 1 dollar if it is zero */
		tempvar income
		gen `inctype' `income' = cond(`inc',`inc',1) if `touse'
		markout `touse' `yname' `income'

		_crcnuse `touse'
		local N `r(N)'
		local first=r(first)
		local last=r(last)
		local in "in `first'/`last'"
		local ifgaps=r(gaps)
		local if = cond(`ifgaps', "if `touse'", "")

		global S_1 `mtr'
		global S_2 `year'
		global S_3 `income'
		global S_4 `if'
		global S_5 `in'

		gen `type' `mtr' = . $S_4 $S_5
		mtr1
		mtr2
		mtr3
		mtr4
		mtr5
		mtr6
		mtr7
	}
end

