/*
	INCLUDE for e() option of predict in PROGRAM part of dlg

	VERSION 1.0.0  21dec2002
*/

	beginoptions
		if main.rb_e {
			put "e("
			if main.en_e_a {
				put main.en_e_a
			}
			if !main.en_e_a {
				put "."
			}
			put ","
			if main.en_e_b {
				put main.en_e_b
			}
			if !main.en_e_b {
				put "."
			}
			put ")"
		}
	endoptions

