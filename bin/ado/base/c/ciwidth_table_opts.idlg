PROGRAM custom_on_done
BEGIN
	call labelCustom_FormatVec.store class				///
		custom_col_pos.value class				///
		labelCustomResult.value
	call widthCustom_FormatVec.store class				///
		custom_col_pos.value class				///
		widthCustomResult.value
	call formatCustom_FormatVec.store class				///
		custom_col_pos.value class				///
		formatCustomResult.value
	call table_custom_StatusVec.store class				///
		custom_col_pos.value "1"
	call table_custom_ResultVec.store class				///
		custom_col_pos.value class				///
		CustomColSpecResult.value

	if CustomColSpecResult.isneq("") {
	call custom_col_pos.withvalue selected_custom_tmp.setvalue class  ///
			table_customVec_origin.values[@]
		call selected_custom_tmp.withvalue			///
			table_customDisplayVec.store			///
			class custom_col_pos.value "@ (On) *"

	call selected_custom_tmp.withvalue selected_custom_tmp2.setvalue ///
			"@ (On) *"
	}
	else {
	call custom_col_pos.withvalue selected_custom_tmp.setvalue class  ///
			table_customVec_origin.values[@]
		call selected_custom_tmp.withvalue			///
			table_customDisplayVec.store			///
			class custom_col_pos.value "@ (On)"

	call selected_custom_tmp.withvalue selected_custom_tmp2.setvalue ///
			"@ (On)"
	}

	call table_customDisplayVec.copyToArray table_customList
	call table_opts.lb_custom.repopulate
	call table_opts.lb_custom.setvalue class selected_custom_tmp2.value

	call CustomColSpecResult.withvalue				///
		table_opts.tx_custom_note.setlabel "@"
END

