	tooltip(resource LBL_TABLE_OPTS_TABLE_OPTS_BU_CUSTOM_UP_TOOLTIP)
	BUTTON   bu_custom_down	@	_ms	100	.,		///
		label(resource LBL_DOWN)				///
		onpush(program custom_down_spec)			///
	tooltip(resource LBL_TABLE_OPTS_TABLE_OPTS_BU_CUSTOM_DOWN_TOOLTIP)
	TEXT	 tx_custom_note	_iilft	198	tablewidth	.,	///
