PROGRAM table_create4Onemean
BEGIN
	call script _table_SETUP
	call table_dfltList.Arrdropall
	call table_addList.Arrdropall
	call table_customList.Arrdropall
	call labelDflt_FormatList.Arrdropall
	call widthDflt_FormatList.Arrdropall
	call formatDflt_FormatList.Arrdropall
	call labelAdd_FormatList.Arrdropall
	call widthAdd_FormatList.Arrdropall
	call formatAdd_FormatList.Arrdropall
	call labelCustom_FormatList.Arrdropall
	call widthCustom_FormatList.Arrdropall
	call formatCustom_FormatList.Arrdropall
	call table_default_ResultList.Arrdropall
	call table_add_ResultList.Arrdropall
	call table_custom_ResultList.Arrdropall
	call table_custom_StatusList.Arrdropall
	call table_add_StatusList.Arrdropall
	call table_dfltList_values.Arrdropall
	call table_addList_values.Arrdropall
	call table_customList_values.Arrdropall
	call table_custom_dlgList.Arrdropall

//Default
	if main.cb_confidence.iseq("level") {
		call table_dfltList.ArrpushR resource dlg LBL_TX_LEVEL
		call table_dfltList_values.Arrpush "level"
	} 
	else {
		call table_dfltList.ArrpushR resource dlg LBL_SIGNIFICANCE_LEVEL
		call table_dfltList_values.Arrpush "alpha"
	}
	call table_dfltList.ArrpushR resource dlg LBL_NUMBER_OF_SUBJECTS //n
	call table_dfltList_values.Arrpush "N"

	if main.cb_compute.isneq("probci") & !main.ck_knownsd {
		call table_dfltList.ArrpushR resource dlg	///
			LBL_PROBABILITY_OF_CI_WIDTH
		call table_dfltList_values.Arrpush "Pr_width"
	}

	if main.cb_compute.isneq("ciwidth") {
		call table_dfltList.ArrpushR resource dlg LBL_CI_WIDTH
		call table_dfltList_values.Arrpush "width"
	}
/*
	if main.ed_mean & main.ed_mean.isvisible() {
		call table_dfltList.ArrpushR "Lower limit"
//		call table_dfltList.ArrpushR resource dlg LBL_LOWER_LIMIT
		call table_dfltList_values.Arrpush "ll"
		call table_dfltList.ArrpushR "Upper limit"
//		call table_dfltList.ArrpushR resource dlg LBL_UPPER_LIMIT
		call table_dfltList_values.Arrpush "ul"
		call table_dfltList.ArrpushR resource LBL_MEAN
		call table_dfltList_values.Arrpush "m"
	}
*/

	call table_dfltList.ArrpushR resource dlg LBL_STANDARD_DEVIATION
	call table_dfltList_values.Arrpush "sd"

	call arraySize.storeClsArraySize table_dfltList

	if arraySize.isgt(10) {
		call arraySize.setvalue 10
	}
	call loopI.setvalue 1

	while(loopI.isle(arraySize)) {
		call labelDflt_FormatList.Arrpush ""
		call widthDflt_FormatList.Arrpush ""
		call formatDflt_FormatList.Arrpush ""
		call table_default_ResultList.Arrpush ""
		call loopI.increment
	}
//Add
	if main.cb_confidence.iseq("level") {
		call table_addList.ArrpushR resource dlg LBL_SIGNIFICANCE_LEVEL
		call table_addList_values.Arrpush "alpha"
	}
	else {
		call table_addList.ArrpushR resource dlg LBL_TX_LEVEL
		call table_addList_values.Arrpush "level"
	}
/*
	if main.cb_compute.isneq("probci") {
		call table_addList.ArrpushR "Probability of CI width"
//		call table_addList.ArrpushR resource dlg		///
//				LBL_PROBABILITY_OF_CI_WIDTH
		call table_addList_values.Arrpush "Pr_width"
	}

	if main.cb_compute.isneq("ciwidth") {
		call table_addList.ArrpushR "CI width"
//		call table_addList.ArrpushR resource dlg LBL_CI_WIDTH	
		call table_addList_values.Arrpush "width"
	}
*/
	call arraySize.storeClsArraySize table_addList
	if arraySize.isgt(10) {
		call arraySize.setvalue 10
	}
	call loopI.setvalue 1

	while(loopI.isle(arraySize)) {
		call labelAdd_FormatList.Arrpush ""
		call widthAdd_FormatList.Arrpush ""
		call formatAdd_FormatList.Arrpush ""
		call table_add_ResultList.Arrpush ""
		call table_add_StatusList.Arrpush "0"
		call loopI.increment
	}

//Custom
	call table_customList.ArrpushR resource dlg LBL_TX_LEVEL
	call table_customList_values.Arrpush "level"
	call table_customList.ArrpushR resource	dlg LBL_SIGNIFICANCE_LEVEL
	call table_customList_values.Arrpush "alpha"
	call table_customList.ArrpushR resource dlg LBL_NUMBER_OF_SUBJECTS
	call table_customList_values.Arrpush "N"
	if main.cb_compute.isneq("probci") & !main.ck_knownsd {
	call table_customList.ArrpushR resource dlg LBL_PROBABILITY_OF_CI_WIDTH
		call table_customList_values.Arrpush "Pr_width"
	}
	call table_customList.ArrpushR resource dlg LBL_CI_WIDTH
	call table_customList_values.Arrpush "width"
/*
	if main.ed_mean & main.ed_mean.isvisible() {
		call table_customList.ArrpushR "Lower limit"
//		call table_customList.ArrpushR resource dlg LBL_LOWER_LIMIT
		call table_customList_values.Arrpush "ll"
		call table_customList.ArrpushR "Upper limit"
//		call table_customList.ArrpushR resource dlg LBL_UPPER_LIMIT
		call table_customList_values.Arrpush "ul"
		call table_customList.ArrpushR resource LBL_MEAN
		call table_customList_values.Arrpush "m"
	}
*/
	call table_customList.ArrpushR resource dlg LBL_STANDARD_DEVIATION
	call table_customList_values.Arrpush "sd"

	call arraySize.storeClsArraySize table_customList
	if arraySize.isgt(10) {
		call arraySize.setvalue 10
	}
	call loopI.setvalue 1

	while(loopI.isle(arraySize)) {
		call table_custom_StatusList.Arrpush "0"
		call labelCustom_FormatList.Arrpush ""
		call widthCustom_FormatList.Arrpush ""
		call formatCustom_FormatList.Arrpush ""
		call table_custom_ResultList.Arrpush ""
		
		call loopI.withvalue table_custom_dlgList.Arrpush "Custom@"
		call loopI.increment
	}

	if main.cb_fpc.isneq("none") {
		if main.cb_fpc.iseq("pop_size") {
		call table_dfltList.ArrpushR resource dlg LBL_POPULATION_SIZE
		call table_customList.ArrpushR resource dlg LBL_POPULATION_SIZE
		}
		else {
		call table_dfltList.ArrpushR resource dlg LBL_SAMPLING_RATE
		call table_customList.ArrpushR resource dlg LBL_SAMPLING_RATE
		}
		call labelDflt_FormatList.Arrpush ""
		call widthDflt_FormatList.Arrpush ""
		call formatDflt_FormatList.Arrpush ""
		call table_default_ResultList.Arrpush ""
		call table_custom_StatusList.Arrpush "0"
		call labelCustom_FormatList.Arrpush ""
		call widthCustom_FormatList.Arrpush ""
		call formatCustom_FormatList.Arrpush ""
		call table_custom_ResultList.Arrpush ""
		call table_dfltList_values.Arrpush "fpc"
		call table_customList_values.Arrpush "fpc"
		call table_custom_dlgList.Arrpush "Custom10"
	}
	
	call table_dfltVec.copyFromArray table_dfltList
	call table_addVec.copyFromArray table_addList
	call table_customVec.copyFromArray table_customList
	call table_dfltDisplayVec.copyFromArray table_dfltList
	call table_addDisplayVec.copyFromArray table_addList
	call table_customDisplayVec.copyFromArray table_customList
	call table_custom_StatusVec.copyFromArray table_custom_StatusList
	call table_add_StatusVec.copyFromArray table_add_StatusList
	call labelDflt_FormatVec.copyFromArray labelDflt_FormatList
	call labelAdd_FormatVec.copyFromArray labelAdd_FormatList
	call widthAdd_FormatVec.copyFromArray widthAdd_FormatList
	call formatAdd_FormatVec.copyFromArray formatAdd_FormatList
	call labelCustom_FormatVec.copyFromArray labelCustom_FormatList
	call widthCustom_FormatVec.copyFromArray widthCustom_FormatList
	call formatCustom_FormatVec.copyFromArray formatCustom_FormatList
	call table_custom_ResultVec.copyFromArray table_custom_ResultList
	call table_default_ResultVec.copyFromArray table_default_ResultList
	call table_add_ResultVec.copyFromArray table_add_ResultList
	call table_dfltVec_values.copyFromArray table_dfltList_values
	call table_addVec_values.copyFromArray table_addList_values
	call table_customVec_values.copyFromArray table_customList_values
	call table_custom_dlgVec.copyFromArray table_custom_dlgList
	call table_customVec_origin.copyFromArray table_customList
	call table_opts.lb_default.repopulate
	call table_opts.lb_addcol.repopulate
	call table_opts.lb_custom.repopulate
END

