PROGRAM custom_sel_changed
BEGIN
	call table_customDisplayVec.findstr class table_opts.lb_custom.value
	call custom_col_pos.setvalue class table_customDisplayVec.position
	call custom_col_pos.withvalue selected_custom.setvalue class	///
		table_customVec_origin.values[@]
	call custom_col_pos.withvalue selected_custom_values.setvalue class ///
		table_customVec_values.values[@]
	call custom_col_pos.withvalue selected_custom_tmp.setvalue class ///
		table_custom_ResultVec.values[@]

	call custom_vec_size.setvalue class table_customVec.size
	if custom_col_pos.iseq(1) {
		if custom_vec_size.iseq(custom_col_pos) {
			call table_opts.bu_custom_up.disable
			call table_opts.bu_custom_down.disable
		}
		else {
			call table_opts.bu_custom_up.disable
			call table_opts.bu_custom_down.enable
		}
	}
	if custom_col_pos.isgt(1) {
		if custom_vec_size.iseq(custom_col_pos) {
			call table_opts.bu_custom_up.enable
			call table_opts.bu_custom_down.disable
		}
		else {
			call table_opts.bu_custom_up.enable
			call table_opts.bu_custom_down.enable
		}
	}
	call custom_col_pos.withvalue custom_status_tmp.setvalue	///
		class table_custom_StatusVec.values[@]

	if  custom_status_tmp.iseq("1") {
		call table_opts.bu_custom_off.enable
	}
	else {
		call table_opts.bu_custom_off.disable
	}

	call selected_custom_tmp.withvalue				///
		table_opts.tx_custom_note.setlabel "@"
END

PROGRAM colspec_default_table
BEGIN
	call labelDflt_FormatVec.copyToString DfltLabelSpecResult
	call widthDflt_FormatVec.copyToString DfltWidthSpecResult
	call formatDflt_FormatVec.copyToString DfltFormatSpecResult
	call labelAdd_FormatVec.copyToString AddLabelSpecResult
	call widthAdd_FormatVec.copyToString AddWidthSpecResult
	call formatAdd_FormatVec.copyToString AddFormatSpecResult
	if table_opts.ck_add {
		put AddLabelSpecResult
	}
	beginoptions	
		if table_opts.ck_add {
			put " add "
		}
		if DfltLabelSpecResult {
			put "labels("
			put DfltLabelSpecResult
			put ")"
		}
		if DfltWidthSpecResult | AddWidthSpecResult {
			put " widths("
			put DfltWidthSpecResult
			if table_opts.ck_add & AddWidthSpecResult {
				put " " AddWidthSpecResult
			}
			put ")"
		}
		if DfltFormatSpecResult | AddFormatSpecResult {
			put " formats("
			put DfltFormatSpecResult
			if table_opts.ck_add & AddFormatSpecResult {
				put " " AddFormatSpecResult
			}
			put ")"
		}
		option table_opts.ck_nofmt
		optionarg /hidedefault table_opts.sp_sep
		option table_opts.ck_divider
		option table_opts.ck_byrow
	endoptions
END

PROGRAM colspec_custom_table
BEGIN
	call labelCustom_FormatVec.copyToString CustomLabelSpecResult
	call widthCustom_FormatVec.copyToString CustomWidthSpecResult
	call formatCustom_FormatVec.copyToString CustomFormatSpecResult
	put CustomLabelSpecResult
	beginoptions
		if CustomWidthSpecResult {
			put "widths("
			put CustomWidthSpecResult
			put ")"
		}
		if CustomFormatSpecResult {
			put " formats("
			put CustomFormatSpecResult
			put ")"
		}
		option table_opts.ck_nofmt
		optionarg /hidedefault table_opts.sp_sep
		option table_opts.ck_divider
		option table_opts.ck_byrow
	endoptions
END

PROGRAM colspec_all_table
BEGIN
