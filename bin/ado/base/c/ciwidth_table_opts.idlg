LIST	produce_list
BEGIN
	resource LBL_TABLE_OPTS_DEFAULT_OUTPUT
	resource LBL_TABLE_OPTS_DEFAULT_TABLE
	resource LBL_TABLE_OPTS_CUSTOM_TABLE
	resource LBL_TABLE_OPTS_SUPPRESS_TABLE
END

LIST	produce_values
BEGIN
	output
	dtable
	ctable
	stable
END

PROGRAM	produce_change
BEGIN
	if table_opts.cb_produce.iseq("output") {
		call script table_off
	}
	if table_opts.cb_produce.iseq("dtable") {
		call program default_table_on
	}	
	if table_opts.cb_produce.iseq("ctable") {
		call script custom_table_on
	}
	if table_opts.cb_produce.iseq("stable") {
		call program suppress_table_on
	}
END

PROGRAM default_table_on
BEGIN
	call table_opts.tx_default.show
	call table_opts.lb_default.show
	call table_opts.bu_edit.show
	call table_opts.tx_note.show
	call table_opts.ck_add.show
	call table_opts.lb_addcol.show
	
	if table_opts.ck_add {
		call table_opts.bu_editadd.show
		call table_opts.bu_editrm.show
	}
	else {
		call table_opts.bu_editadd.hide
		call table_opts.bu_editrm.hide		
	}
	call table_opts.gb_table.show
	call script custom_table_off
	call script common_opts_on
	call script opt_save_show

	call table_opts.ck_add.setposition . 218
	call table_opts.lb_addcol.setposition . 240
	call table_opts.bu_editadd.setposition . 240
	call table_opts.bu_editrm.setposition . 265
	call table_opts.ck_nofmt.setposition . 300
	call table_opts.ck_divider.setposition . 300
	call table_opts.ck_byrow.setposition . 325
	call table_opts.sp_sep.setposition . 350
	call table_opts.tx_sep.setposition . 350
	
	call table_opts.ck_save.setposition . 395
	call table_opts.fi_save.setposition . 420
	call table_opts.ck_repdta.setposition . 445
END

SCRIPT default_table_off
BEGIN
	table_opts.gb_table.hide
	table_opts.tx_default.hide
	table_opts.lb_default.hide
	table_opts.bu_edit.hide
	table_opts.tx_note.hide
	table_opts.ck_add.hide
	table_opts.lb_addcol.hide
	table_opts.bu_editadd.hide
	table_opts.bu_editrm.hide
END

SCRIPT custom_table_on
BEGIN
	table_opts.gb_custom_table.show
	table_opts.lb_custom.show
	table_opts.bu_custom_on.show
	table_opts.bu_custom_off.show
	table_opts.bu_custom_up.show
	table_opts.bu_custom_down.show
	table_opts.tx_custom_note.show
	table_opts.rb_custom.show
	table_opts.rb_all.show
	table_opts.tx_allwidth.show
	table_opts.ed_allwidth.show
	table_opts.tx_allfmt.show
	table_opts.ed_allfmt.show
	script default_table_off
	script common_opts_on
	script opt_save_show

	table_opts.tx_allwidth.setposition . 245
	table_opts.ed_allwidth.setposition . 245
	table_opts.tx_allfmt.setposition . 245
	table_opts.ed_allfmt.setposition . 245
	table_opts.ck_nofmt.setposition . 280
	table_opts.ck_divider.setposition . 280
	table_opts.ck_byrow.setposition . 305
	table_opts.sp_sep.setposition . 330
	table_opts.tx_sep.setposition . 330
	
	table_opts.ck_save.setposition . 375
	table_opts.fi_save.setposition . 400
	table_opts.ck_repdta.setposition . 425
END

SCRIPT custom_table_off
BEGIN
	table_opts.gb_custom_table.hide
	table_opts.lb_custom.hide
	table_opts.bu_custom_on.hide
	table_opts.bu_custom_off.hide
	table_opts.bu_custom_up.hide
	table_opts.bu_custom_down.hide
	table_opts.tx_custom_note.hide
	table_opts.rb_custom.hide
	table_opts.rb_all.hide
	table_opts.tx_allwidth.hide
	table_opts.ed_allwidth.hide
	table_opts.tx_allfmt.hide
	table_opts.ed_allfmt.hide
END

SCRIPT common_opts_on
BEGIN
	table_opts.ck_nofmt.show
	table_opts.ck_divider.show
	table_opts.ck_byrow.show
	table_opts.sp_sep.show
	table_opts.tx_sep.show
END

SCRIPT common_opts_off
BEGIN
	table_opts.ck_nofmt.hide
	table_opts.ck_divider.hide
	table_opts.ck_byrow.hide
	table_opts.sp_sep.hide
	table_opts.tx_sep.hide
END

SCRIPT table_off
BEGIN
	script default_table_off
	script custom_table_off
	script common_opts_off
	script opt_save_hide
END

PROGRAM suppress_table_on
BEGIN
	call script default_table_off
	call script custom_table_off
	call script common_opts_off
	call script opt_save_show
	
	call table_opts.ck_save.setposition . 70
	call table_opts.fi_save.setposition . 95
	call table_opts.ck_repdta.setposition . 120	
END

SCRIPT table_ck_add_on
BEGIN
	table_opts.lb_addcol.enable
	table_opts.bu_editadd.show
	table_opts.bu_editrm.show
END

SCRIPT table_ck_add_off
BEGIN
	table_opts.lb_addcol.disable
	table_opts.bu_editadd.hide
	table_opts.bu_editrm.hide
END

PROGRAM table_all_on
BEGIN
	call table_opts.tx_allwidth.enable
	call table_opts.ed_allwidth.enable
	call table_opts.tx_allfmt.enable
	call table_opts.ed_allfmt.enable
	
	call table_opts.lb_custom.disable
	call table_opts.bu_custom_on.hide
	call table_opts.bu_custom_off.hide
	call table_opts.bu_custom_up.hide
	call table_opts.bu_custom_down.hide
	call table_opts.tx_custom_note.hide
END

PROGRAM table_all_off
BEGIN
	call table_opts.tx_allwidth.disable
	call table_opts.ed_allwidth.disable
	call table_opts.tx_allfmt.disable
	call table_opts.ed_allfmt.disable
	
	call table_opts.lb_custom.enable
	if table_opts.cb_produce.iseq("ctable") & table_opts.rb_custom { 
		call table_opts.bu_custom_on.show
		call table_opts.bu_custom_off.show
		call table_opts.bu_custom_up.show
		call table_opts.bu_custom_down.show
		call table_opts.tx_custom_note.show
	}
END

SCRIPT opt_save_on
BEGIN
	table_opts.fi_save.enable
	table_opts.ck_repdta.enable
END

SCRIPT opt_save_off
BEGIN
	table_opts.fi_save.disable
	table_opts.ck_repdta.disable
END

SCRIPT opt_save_show
BEGIN
	table_opts.ck_save.show
	table_opts.fi_save.show
	table_opts.ck_repdta.show
END

SCRIPT opt_save_hide
BEGIN
	table_opts.ck_save.hide
	table_opts.fi_save.hide
	table_opts.ck_repdta.hide
END

PROGRAM show_default_spec
BEGIN
	call table_dfltDisplayVec.findstr class table_opts.lb_default.value
	call dflt_col_pos.setvalue class table_dfltDisplayVec.position
	call dflt_col_pos.withvalue dflt_childdlg.setvalue "Default@"

	call dflt_childdlg.withvalue create CHILD power_table_default_spec AS @
	call dflt_childdlg.withvalue @.setExitString DfltColSpecResult
	call dflt_childdlg.withvalue @.setExitAction "program default_edit_done"
END

PROGRAM default_edit_done
BEGIN

	call labelDflt_FormatVec.store class				///
		dflt_col_pos.value class				///
		labelDfltResult.value
	call widthDflt_FormatVec.store class				///
		dflt_col_pos.value class				///
		widthDfltResult.value
	call formatDflt_FormatVec.store class				///
		dflt_col_pos.value class				///
		formatDfltResult.value
	call table_default_ResultVec.store class			///
		dflt_col_pos.value class				///
		DfltColSpecResult.value

	if DfltColSpecResult.isneq("") {
		call dflt_col_pos.withvalue				///
			selected_dflt_tmp.setvalue class		///
			table_dfltVec.values[@]
		call selected_dflt_tmp.withvalue			///
			table_dfltDisplayVec.store			///
			class dflt_col_pos.value "@ *"

		call table_dfltDisplayVec.copyToArray table_dfltList
		call selected_dflt_tmp.withvalue			///
				selected_dflt_tmp2.setvalue "@ *"
		call table_opts.lb_default.repopulate
		call table_opts.lb_default.setvalue			///
			class selected_dflt_tmp2.value
	}
	else {
		call dflt_col_pos.withvalue				///
			selected_dflt_tmp.setvalue class		///
			table_dfltVec.values[@]
		call selected_dflt_tmp.withvalue			///
			table_dfltDisplayVec.store			///
			class dflt_col_pos.value "@"

		call table_dfltDisplayVec.copyToArray table_dfltList
		call selected_dflt_tmp.withvalue			///
			selected_dflt_tmp2.setvalue "@"
		call table_opts.lb_default.repopulate
		call table_opts.lb_default.setvalue			///
			class selected_dflt_tmp2.value
	}

	call DfltColSpecResult.withvalue table_opts.tx_note.setlabel "@"
END

PROGRAM default_sel_changed
BEGIN
	call table_dfltDisplayVec.findstr class table_opts.lb_default.value
	call dflt_col_pos.setvalue class table_dfltDisplayVec.position
	call dflt_col_pos.withvalue selected_dflt.setvalue class	///
		table_dfltVec.values[@]
	call dflt_col_pos.withvalue selected_dflt_values.setvalue class ///
		table_dfltVec_values.values[@]

	call dflt_col_pos.withvalue selected_dflt_tmp3.setvalue class	///
		table_default_ResultVec.values[@]
	call selected_dflt_tmp3.withvalue				///
		table_opts.tx_note.setlabel "@"
END

PROGRAM add_on_spec
BEGIN
	call table_addDisplayVec.findstr class table_opts.lb_addcol.value
	call add_col_pos.setvalue class table_addDisplayVec.position
	call add_col_pos.withvalue add_childdlg.setvalue		///
		"Add@"

	call add_childdlg.withvalue create CHILD power_table_add_spec AS @
	call add_childdlg.withvalue @.setExitString AddColSpecResult
	call add_childdlg.withvalue @.setExitAction "program add_on_done"
END

PROGRAM add_on_done
BEGIN
	call labelAdd_FormatVec.store class				///
		add_col_pos.value class					///
		labelAddResult.value
	call widthAdd_FormatVec.store class				///
		add_col_pos.value class					///
		widthAddResult.value
	call formatAdd_FormatVec.store class				///
		add_col_pos.value class					///
		formatAddResult.value
	call table_add_StatusVec.store class				///
		add_col_pos.value "1"
	call table_add_ResultVec.store class				///
		add_col_pos.value class					///
		AddColSpecResult.value

	if AddColSpecResult.isneq("") {
		call add_col_pos.withvalue selected_add_tmp.setvalue class  ///
			table_addVec.values[@]
		call selected_add_tmp.withvalue				///
			table_addDisplayVec.store			///
			class add_col_pos.value "@ (On) *"

		call selected_add_tmp.withvalue selected_add_tmp2.setvalue ///
			"@ (On) *"
	}
	else {
		call add_col_pos.withvalue selected_add_tmp.setvalue class  ///
			table_addVec.values[@]
		call selected_add_tmp.withvalue				///
			table_addDisplayVec.store			///
			class add_col_pos.value "@ (On)"

		call selected_add_tmp.withvalue selected_add_tmp2.setvalue ///
			"@ (On)"
