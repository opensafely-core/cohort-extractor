//				twoway_tsline_parse
//
// A parser for time-series line graphs.  Works with twoway.ado.

*/
*! version 1.0.1  02sep2003

version 8

class {
} , inherit(twoway_yxview_parse)

// ---------------------------------------------------------------------------

program parse
	.viewtype = "line"

	syntax anything(id="varlist" equalok)		///
		[aw fw pw ] [if] [in] [,		///
			ONEPanel			///
			SORT				/// ignored
			SORT1(namelist)			/// ignored
			*				///
		]

	marksample touse

	capture  _ts t ignored if `touse', onepanel
	if c(rc) _ts t ignored, panel

	.Super.parse  `anything' `t' [`weight'`exp']	///
		`if' `in', sort(`t') `options'
end

exit
