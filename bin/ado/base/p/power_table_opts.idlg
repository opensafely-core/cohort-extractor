PROGRAM power_save_output
BEGIN
	if table_opts.ck_save {
		require table_opts.fi_save
		put `"saving("'
		put table_opts.fi_save
		if table_opts.ck_repdta {
			put `", replace"'
		}
		put `")"'
	}
END


PROGRAM power_table_opts_output
BEGIN
	call table_dfltDisplayVec.copyToArray tmp_dfltDisplayList
	call tmp_dfltDisplayVec.copyFromArray tmp_dfltDisplayList
	call labelDflt_FormatVec.copyToArray tmp_labelDflt_FormatList
	call tmp_labelDflt_FormatVec.copyFromArray tmp_labelDflt_FormatList
	call widthDflt_FormatVec.copyToArray tmp_widthDflt_FormatList
	call tmp_widthDflt_FormatVec.copyFromArray tmp_widthDflt_FormatList
	call formatDflt_FormatVec.copyToArray tmp_formatDflt_FormatList
	call tmp_formatDflt_FormatVec.copyFromArray tmp_formatDflt_FormatList
	call table_default_ResultVec.copyToArray tmp_default_ResultList
	call tmp_default_ResultVec.copyFromArray tmp_default_ResultList

	call table_addDisplayVec.copyToArray tmp_addDisplayList
	call tmp_addDisplayVec.copyFromArray tmp_addDisplayList
	call table_add_StatusVec.copyToArray tmp_add_StatusList
	call tmp_add_StatusVec.copyFromArray tmp_add_StatusList
	call labelAdd_FormatVec.copyToArray tmp_labelAdd_FormatList
	call tmp_labelAdd_FormatVec.copyFromArray tmp_labelAdd_FormatList
	call widthAdd_FormatVec.copyToArray tmp_widthAdd_FormatList
	call tmp_widthAdd_FormatVec.copyFromArray tmp_widthAdd_FormatList
	call formatAdd_FormatVec.copyToArray tmp_formatAdd_FormatList
	call tmp_formatAdd_FormatVec.copyFromArray tmp_formatAdd_FormatList
	call table_add_ResultVec.copyToArray tmp_add_ResultList
	call tmp_add_ResultVec.copyFromArray tmp_add_ResultList
	
	call table_customDisplayVec.copyToArray tmp_customDisplayList
	call tmp_customDisplayVec.copyFromArray tmp_customDisplayList
	call table_custom_StatusVec.copyToArray tmp_custom_StatusList
	call tmp_custom_StatusVec.copyFromArray tmp_custom_StatusList
	call labelCustom_FormatVec.copyToArray tmp_labelCustom_FormatList
	call tmp_labelCustom_FormatVec.copyFromArray tmp_labelCustom_FormatList
	call widthCustom_FormatVec.copyToArray tmp_widthCustom_FormatList
	call tmp_widthCustom_FormatVec.copyFromArray tmp_widthCustom_FormatList
	call formatCustom_FormatVec.copyToArray tmp_formatCustom_FormatList
	call tmp_formatCustom_FormatVec.copyFromArray tmp_formatCustom_FormatList
	call table_custom_ResultVec.copyToArray tmp_custom_ResultList
	call tmp_custom_ResultVec.copyFromArray tmp_custom_ResultList
	call table_custom_dlgVec.copyToArray tmp_custom_dlgList
	call tmp_custom_dlgVec.copyFromArray tmp_custom_dlgList
	call table_customVec_origin.copyToArray tmp_customList_origin
	call tmp_customVec_origin.copyFromArray tmp_customList_origin
	call table_customVec_values.copyToArray tmp_customList_values				
	call tmp_customVec_values.copyFromArray tmp_customList_values

	if table_opts.cb_produce.iseq("dtable") {
		call labelDflt_FormatVec.copyToString DfltLabelSpecResult
		call widthDflt_FormatVec.copyToString DfltWidthSpecResult
		call formatDflt_FormatVec.copyToString DfltFormatSpecResult
		call labelAdd_FormatVec.copyToString AddLabelSpecResult
		call widthAdd_FormatVec.copyToString AddWidthSpecResult
		call formatAdd_FormatVec.copyToString AddFormatSpecResult
		
		if table_opts.ck_add {
			if !AddLabelSpecResult & !AddWidthSpecResult & !AddFormatSpecResult {	
				call create STRING _MSG_TMP
				call _MSG_TMP.format 				///
					"resource LBL_TABLE_OPTS_MSGBOX1_FMT"	///
					"resource LBL_TABLE"
				stopbox stop "class _MSG_TMP.value"
// On the "Table" tab, columns to be added to the default table must be chosen.
			}
		}
		
		if DfltLabelSpecResult | DfltWidthSpecResult | DfltFormatSpecResult |	///
			table_opts.ck_add | table_opts.ck_nofmt | table_opts.sp_sep.isneq(0) | ///
			table_opts.ck_divider | table_opts.ck_byrow {
			put "table("
			put /program colspec_default_table
			put ")"
		} 
		else {
			put " table"
		}
	}
	
	if table_opts.cb_produce.iseq("stable") {
		put " notable"
	}
	if table_opts.cb_produce.iseq("ctable") & table_opts.rb_custom {
		call labelCustom_FormatVec.copyToString CustomLabelSpecResult
		call widthCustom_FormatVec.copyToString CustomWidthSpecResult
		call formatCustom_FormatVec.copyToString CustomFormatSpecResult
		
		if !CustomLabelSpecResult & !CustomWidthSpecResult & !CustomFormatSpecResult {
			call create STRING _MSG_TMP
			call _MSG_TMP.format 				///
				"resource LBL_TABLE_OPTS_MSGBOX2_FMT"	///
				"resource LBL_TABLE"
			stopbox stop "class _MSG_TMP.value"
// On the "Table" tab, columns for the custom table must be chosen.
		}
		
		if CustomLabelSpecResult | CustomWidthSpecResult |	///
			CustomFormatSpecResult | 			///
			table_opts.ck_nofmt |				///
			table_opts.sp_sep.isneq(0) |			///
			table_opts.ck_divider | table_opts.ck_byrow {
			put "table("
			put /program colspec_custom_table
			put ")"
		}
	}
	
	if table_opts.cb_produce.iseq("ctable") & table_opts.rb_all {
		put "table(_all"
		put /program colspec_all_table
		put ")"
	}
	
	put " " /program power_save_output
END

