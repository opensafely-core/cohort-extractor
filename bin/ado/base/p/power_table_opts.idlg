PROGRAM default_edit_done
BEGIN

	call labelDflt_FormatVec.store class				///
		dflt_col_pos.value class				///
		labelDfltResult.value
	call widthDflt_FormatVec.store class				///
		dflt_col_pos.value class				///
		widthDfltResult.value
	call formatDflt_FormatVec.store class				///
		dflt_col_pos.value class				///
		formatDfltResult.value
	call table_default_ResultVec.store class			///
		dflt_col_pos.value class				///
		DfltColSpecResult.value

	if DfltColSpecResult.isneq("") {
		call dflt_col_pos.withvalue selected_dflt_tmp.setvalue class  ///
			table_dfltVec.values[@]
		call selected_dflt_tmp.withvalue			///
			table_dfltDisplayVec.store			///
			class dflt_col_pos.value "@ *"

		call table_dfltDisplayVec.copyToArray table_dfltList
		call selected_dflt_tmp.withvalue selected_dflt_tmp2.setvalue ///
			"@ *"
		call table_opts.lb_default.repopulate
		call table_opts.lb_default.setvalue class selected_dflt_tmp2.value
	}
	else {
		call dflt_col_pos.withvalue selected_dflt_tmp.setvalue class  ///
			table_dfltVec.values[@]
		call selected_dflt_tmp.withvalue			///
			table_dfltDisplayVec.store			///
			class dflt_col_pos.value "@"

		call table_dfltDisplayVec.copyToArray table_dfltList
		call selected_dflt_tmp.withvalue selected_dflt_tmp2.setvalue ///
			"@"
		call table_opts.lb_default.repopulate
		call table_opts.lb_default.setvalue class selected_dflt_tmp2.value
	}

	call DfltColSpecResult.withvalue table_opts.tx_note.setlabel "@"
END

