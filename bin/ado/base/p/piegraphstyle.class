//				piegraphstyle
//
//  Base style for pie graphs

*! version 1.0.0  08dec2002
version 8

class {

    instance:

 	intensity	= .intensity.new , style(10)
	linestyle	= .linestyle.new , style(foreground)

	angle0		= .numstyle.new	 , style(90)
	clockwise	= .yesno.new	 , style(yes)

	explode_gap	= .gsize.new	 , style(medium)

//	pielabel_method = .pielabelstyle.new	, style(none)
	pielabel_style	= .textboxstyle.new	, style(small_label)
	pielabel_format = "%6.0g"
	pielabel_gap	= .gsize.new		, style(zero)
	pielabel_pos	= .inoutstyle.new	, style(inside)

} , inherit(graphstyle)
