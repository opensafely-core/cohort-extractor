PROGRAM gr_plotopts_POSTINIT_PROGRAM
BEGIN
	call gr_plotopts_STMP.format "resource LBL_ALL_PLOTS"
	call gr_plotopts_STMP.withvalue gr_plotopts_plotList.Arrpush "@"
	call gr_plotopts_STMP.format "resource LBL_PLOT_FMT" "1" ""
	call gr_plotopts_STMP.withvalue gr_plotopts_plotList.Arrpush "@"
	call gr_plotopts_STMP.format "resource LBL_PLOT_FMT" "2" ""
	call gr_plotopts_STMP.withvalue gr_plotopts_plotList.Arrpush "@"
	call gr_plotopts_STMP.format "resource LBL_PLOT_FMT" "3" ""
	call gr_plotopts_STMP.withvalue gr_plotopts_plotList.Arrpush "@"
	call gr_plotopts_STMP.format "resource LBL_PLOT_FMT" "4" ""
	call gr_plotopts_STMP.withvalue gr_plotopts_plotList.Arrpush "@"
	call gr_plotopts_STMP.format "resource LBL_PLOT_FMT" "5" ""
	call gr_plotopts_STMP.withvalue gr_plotopts_plotList.Arrpush "@"
	call gr_plotopts_STMP.format "resource LBL_PLOT_FMT" "6" ""
	call gr_plotopts_STMP.withvalue gr_plotopts_plotList.Arrpush "@"
	call gr_plotopts_STMP.format "resource LBL_PLOT_FMT" "7" ""
	call gr_plotopts_STMP.withvalue gr_plotopts_plotList.Arrpush "@"
	call gr_plotopts_STMP.format "resource LBL_PLOT_FMT" "8" ""
	call gr_plotopts_STMP.withvalue gr_plotopts_plotList.Arrpush "@"
	call gr_plotopts.lb_plot.repopulate
END

