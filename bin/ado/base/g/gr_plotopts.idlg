	call gr_plotopts_areaOutput.setvalue ""
	call gr_plotopts_barOutput.setvalue ""
	call gr_plotopts_spikeOutput.setvalue ""
	call gr_plotopts_droplineOutput.setvalue ""


	if gr_plotopts.cb_plot_type.iseq("") {
		if (gr_plotopts_conndResults |				///
		gr_plotopts_conn1Results | gr_plotopts_conn2Results |	///
		gr_plotopts_conn3Results | gr_plotopts_conn4Results |	///
