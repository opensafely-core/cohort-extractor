	if main.rb_basic {
		call main.gb_plot.setlabel resource LBL_PLOT_TYPE_SCATTERPLOT
		call script basic_show_common
		call script range_hide_all
		call script contour_hide_all
		call script fit_hide_all
		call script immediate_hide_all
