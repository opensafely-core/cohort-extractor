	if main.rb_range {
		if main.ck_range_sortx | main.ck_range_y2 |		 ///
		 ( ! H(main.bu_range_area) & rangeAreaResults )      |	 ///
		 ( ! H(main.bu_range_bar) & rangeBarResults )        |	 ///
		 ( ! H(main.bu_range_spike) & rangeSpikeResults )    |	 ///
		 ( ! H(main.cb_range_cap_size) & main.cb_range_cap_size ) | ///
		 ( ! H(main.bu_range_scatter) & rangeMarkerResults ) |	 ///
		 ( ! H(main.bu_range_line) & rangeLineResults ) 	 ///
		{
			call PARENT.callthru `"build_CMD.append ", ...""'
		}
		call PARENT.callthru `"build_CMD.append ")""'
	}

	if main.rb_contour {
		if main.ck_con_y2 |					///
		 ( ! H(main.bu_contour) & ContourResults )	|	///
	 	 ( ! H(main.bu_contourline) & ContourLineResults)	/// 
		{
			call PARENT.callthru `"build_CMD.append ", ...""'
		}
		call PARENT.callthru `"build_CMD.append ")""'
	}
	
	if main.rb_fit {
		if main.ck_fit_y2 |					 ///
		 ( ! H(main.bu_fit_lfit) & fitLFitResults )	|	 ///
		 ( ! H(main.bu_fit_qfit) & fitQFitResults )	|	 ///
		 ( ! H(main.bu_fit_fpfit) & fitFPFitResults )	|	 ///
		 ( ! H(main.bu_fit_lfitci) & fitLFitCiResults ) |	 ///
		 ( ! H(main.bu_fit_qfitci) & fitQFitCiResults ) |	 ///
		 ( ! H(main.bu_fit_fpfitci) & fitFPFitCiResults )	 ///
		{
			call PARENT.callthru `"build_CMD.append ", ...""'
		}
		call PARENT.callthru `"build_CMD.append ")""'
	}
	
	if main.rb_immediate {
		if immediateRecast.isneq("")	|			 ///
		 ( ! H(main.bu_im_marker) & imMarkerResults )	|	 ///
		 ( ! H(main.bu_im_line) & imLineResults )	|	 ///
		 ( ! H(main.bu_im_pcspike) & imSpikeResults )	|	 ///
		 ( ! H(main.bu_im_pcarrow) & imArrowResults )	|	 ///
		 ( ! H(main.bu_im_pcmarker) & imPcMarkerResults )	 ///
		{
			call PARENT.callthru `"build_CMD.append ", ""'
			
			if immediateRecast.isneq("") {
				call immediateRecast.withvalue PARENT.callthru `"build_CMD.append "@""'
			}
		 	if main.ck_im_y2 |				 ///
		 	 ( ! H(main.bu_im_marker) & imMarkerResults ) |  ///
		 	 ( ! H(main.bu_im_line) & imLineResults )     |  ///
		 	 ( ! H(main.bu_im_pcspike) & imSpikeResults ) |  ///
		 	 ( ! H(main.bu_im_pcarrow) & imArrowResults ) |	 ///
		 	 ( ! H(main.bu_im_pcmarker) & imPcMarkerResults) ///
			{
				call PARENT.callthru `"build_CMD.append " ...""'
			}
		}
		call PARENT.callthru `"build_CMD.append ")""'
	}
	
	if main.rb_advanced {
		if main.ck_adv_y2 |					///
		 ( ! H(main.bu_adv_tsline) & advTslineResults )  |	///
		 ( ! H(main.bu_adv_mband) & advMbandResults )	 |	///
		 ( ! H(main.ck_adv_bands) & main.ck_adv_bands )	 |	///
		 ( ! H(main.ck_adv_n) & main.ck_adv_n )		 |	///
		 ( ! H(main.bu_adv_lowess) & advLowessResults )  |	///
		 ( ! H(main.bu_adv_lowess_line) & advLowessLineResults ) | ///
		 ( ! H(main.bu_adv_kdensity) & advKdensityResults ) |	///
		 ( ! H(main.bu_adv_kdensity_line) 	///
		 	& advKdensityLineResults ) 		 |	///
		 ( ! H(main.bu_adv_histogram) & advHistogramResults) |	///
		 ( ! H(main.bu_adv_histogram_bar) 	///
		 	& advHistogramBarResults )		 | 	///
		 ( ! H(main.rb_adv_hist_discrete) 	///
		 	& main.rb_adv_hist_discrete )		 |	///
		 ( ! H(main.bu_adv_locpoly) & advLocpolyResults ) |	///
		 ( ! H(main.bu_adv_locpolyci) & advLocpolyCiResults ) |	///
		 ( ! H(main.bu_adv_pcline) & advPcLineResults )	 |	///
		 ( ! H(main.bu_adv_pcmarker) & advPcMarkerResults ) |	///
		 ( ! H(main.bu_adv_pcarrow) & advPcArrowResults )   |	///
		 ( ! H(main.vn_adv_function_range) 	///
		 	& main.vn_adv_function_range ) 		|	///
		 ( ! H(main.bu_adv_function)  	   	///
		 	& (advFunctionResults | advFunctionLineResults )) ///
		{
			call PARENT.callthru `"build_CMD.append ", ...""'
		}
		call PARENT.callthru `"build_CMD.append ")""'
	}
END

SCRIPT get_cmd_if_args
BEGIN
	PARENT.callthru `"build_CMD.append "  if ""'
	sub.ex_if.withvalue PARENT.callthru `"build_CMD.append "@""'
END
SCRIPT get_cmd_in_args
BEGIN
	PARENT.callthru `"build_CMD.append "  in ""'
	sub.sp_from.withvalue PARENT.callthru `"build_CMD.append "@""'
	PARENT.callthru `"build_CMD.append "/""'
	sub.sp_to.withvalue PARENT.callthru `"build_CMD.append "@""'
END

PROGRAM command
BEGIN
	put "("
	put /program gr_plot_output
	put ") "
	
	// update flags in the parent for y axis(2) options
	if main.rb_basic {
		if ! H(main.ck_basic_y2) & main.ck_basic_y2 {
			call PARENT.callthru "build_isY2Set.settrue"
		}
		if H(main.ck_basic_y2) | ! main.ck_basic_y2 {
			call PARENT.callthru "build_isY2Set.setfalse"
		}
	}
	if main.rb_range {
		if ! H(main.ck_range_y2) & main.ck_range_y2 {
			call PARENT.callthru "build_isY2Set.settrue"
		}
		if H(main.ck_range_y2) | ! main.ck_range_y2 {
			call PARENT.callthru "build_isY2Set.setfalse"
		}
	}
	if main.rb_contour {
		if ! H(main.ck_con_y2) & main.ck_con_y2 {
			call PARENT.callthru "build_isY2Set.settrue"
		}
		if H(main.ck_con_y2) | ! main.ck_con_y2 {
			call PARENT.callthru "build_isY2Set.setfalse"
		}
	}
	if main.rb_fit {
		if ! H(main.ck_fit_y2) & main.ck_fit_y2 {
			call PARENT.callthru "build_isY2Set.settrue"
		}
		if H(main.ck_fit_y2) | ! main.ck_fit_y2 {
			call PARENT.callthru "build_isY2Set.setfalse"
		}
	}
	if main.rb_immediate {
		if ! H(main.ck_im_y2) & main.ck_im_y2 {
			call PARENT.callthru "build_isY2Set.settrue"
		}
		if H(main.ck_im_y2) | ! main.ck_im_y2 {
			call PARENT.callthru "build_isY2Set.setfalse"
