PROGRAM main_check_category
BEGIN
	if main.rb_basic {
		call main.lb_type_basic.show
		call main.lb_type_fit.hide
		call main.lb_type_range.hide
		call main.lb_type_contour.hide
		call main.lb_type_immediate.hide
		call main.lb_type_advanced.hide
		call main.tx_category.setlabel resource GR_TW_PLOT_BASIC_SELECT_TYPE
		call main.lb_type_basic.withvalue main.lb_type_basic.setvalue "@"
		call script ifin_enable
	}
	if main.rb_range {
		call main.lb_type_basic.hide
		call main.lb_type_fit.hide
		call main.lb_type_range.show
		call main.lb_type_contour.hide
		call main.lb_type_immediate.hide
		call main.lb_type_advanced.hide
		call main.tx_category.setlabel resource GR_TW_PLOT_RANGE_SELECT_TYPE
		call main.lb_type_range.withvalue main.lb_type_range.setvalue "@"
		call script ifin_enable
	}
	if main.rb_contour {
		call main.lb_type_basic.hide
		call main.lb_type_fit.hide
		call main.lb_type_range.hide
		call main.lb_type_contour.show
		call main.lb_type_immediate.hide
		call main.lb_type_advanced.hide
		call main.tx_category.setlabel resource GR_TW_PLOT_CONTOUR_SELECT_TYPE
		call main.lb_type_contour.withvalue main.lb_type_contour.setvalue "@"
		call script ifin_enable
	}
	if main.rb_fit {
		call main.lb_type_basic.hide
		call main.lb_type_fit.show
		call main.lb_type_range.hide
		call main.lb_type_contour.hide
		call main.lb_type_immediate.hide
		call main.lb_type_advanced.hide
		call main.tx_category.setlabel resource GR_TW_PLOT_FIT_SELECT_TYPE
		call main.lb_type_fit.withvalue main.lb_type_fit.setvalue "@"
		call script ifin_enable
	}	
	if main.rb_immediate {
		call main.lb_type_basic.hide
		call main.lb_type_fit.hide
		call main.lb_type_range.hide
		call main.lb_type_contour.hide
		call main.lb_type_immediate.show
		call main.lb_type_advanced.hide
		call main.tx_category.setlabel resource GR_TW_PLOT_IMMEDIATE_SELECT_TYPE
		call main.lb_type_immediate.withvalue main.lb_type_immediate.setvalue "@"
		call program im_sci_process_list // build and show list of coordinates
		call program im_pci_process_list // build and show list of coordinates
		call script ifin_disable
	}
	if main.rb_advanced {
		call main.lb_type_basic.hide
		call main.lb_type_fit.hide
		call main.lb_type_range.hide
		call main.lb_type_contour.hide
		call main.lb_type_immediate.hide
		call main.lb_type_advanced.show
		call main.tx_category.setlabel resource GR_TW_PLOT_ADVANCED_SELECT_TYPE
		call main.lb_type_advanced.withvalue main.lb_type_advanced.setvalue "@"
		call script ifin_enable
	}	
END

