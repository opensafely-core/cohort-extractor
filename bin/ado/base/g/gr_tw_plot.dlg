INCLUDE ifin

PROGRAM gr_plot_output
BEGIN
	if main.rb_basic {
		put main.lb_type_basic " "
		varlist main.vn_basic_yvar
		varlist main.vn_basic_xvar
		put " " /program ifin_output
		if ! H(main.bu_basic_weight) {
			put " " basicMarkerWeights " "
		}
		beginoptions
			option main.ck_basic_sortx
			option main.ck_basic_y2
			if ! H(main.bu_basic_scatter) {
				put " " basicMarkerResults " "
			}
			if ! H(main.bu_basic_line) {
				put " " basicLineResults " "
			}
			if ! H(main.bu_basic_area) {
				put " " basicAreaResults " "
			}
			if ! H(main.bu_basic_bar) {
				put " " basicBarResults " "
			}
			if ! H(main.bu_basic_spike) {
				put " " basicSpikeResults " "
			}			
		endoptions
	}
	if main.rb_range {
		put main.lb_type_range " "
		varlist main.vn_range_yvar1
		varlist main.vn_range_yvar2
		varlist main.vn_range_xvar
		put " " /program ifin_output
		if ! H(main.bu_range_weight) {
			put " " rangeMarkerWeights " "
		}		
		beginoptions
			option main.ck_range_sortx
			option main.ck_range_y2
			if ! H(main.bu_range_area) {
				put " " rangeAreaResults " "
			}
			if ! H(main.bu_range_bar) {
				put " " rangeBarResults " "
			}
			if ! H(main.bu_range_spike) {
				put " " rangeSpikeResults " "
			}
			optionarg /hidedefault main.cb_range_cap_size
			if ! H(main.bu_range_scatter) {
				put " " rangeMarkerResults " "
			}
			if ! H(main.bu_range_line) {
				put " " rangeLineResults " "
			}
		endoptions
	}

	if main.rb_contour {
		put main.lb_type_contour " "
		varlist main.vn_con_zvar
		varlist main.vn_con_yvar
		varlist main.vn_con_xvar
		put " " /program ifin_output
		beginoptions
			option main.ck_con_y2
			if ! H(main.bu_contour) {
				put " " ContourResults " "
			}
			if ! H(main.bu_contourline) {
				put " " ContourLineResults " "
			}
		endoptions
	}
	
	if main.rb_fit {
		put main.lb_type_fit " "
		varlist main.vn_fit_yvar
		varlist main.vn_fit_xvar
		put " " /program ifin_output
		if ! H(main.bu_fit_weight) {
			put " " fitWeights " "
		}
		beginoptions
			option main.ck_fit_y2
			if ! H(main.bu_fit_lfit) {
				put " " fitLFitResults " "
			}
			if ! H(main.bu_fit_qfit) {
				put " " fitQFitResults " "
			}
			if ! H(main.bu_fit_fpfit) {
				put " " fitFPFitResults " "
			}
			if ! H(main.bu_fit_lfitci) {
				put " " fitLFitCiResults " "
			}
			if ! H(main.bu_fit_qfitci) {
				put " " fitQFitCiResults " "
			}
			if ! H(main.bu_fit_fpfitci) {
				put " " fitFPFitCiResults " "
			}
		endoptions
	}

	if main.rb_immediate {
		if main.lb_type_immediate.contains("scatteri") {
			if  ! (	imSci1 | imSci2 | imSci3 | imSci4 |	///
				imSci5 | imSci6 | imSci7 | imSci8 |	///
				imSci9 | imSci10 | imSci11 | imSci12 |	///
				imSci13 | imSci14 | imSci15 | imSci16 |	///
				imSci17 | imSci18 | imSci19 | imSci20 |	///
				imSci21 | imSci22 | imSci23 | imSci24 |	///
				imSci25 | imSci26 | imSci27 | imSci28 |	///
				imSci29 | imSci30 | imSci31 | imSci32 )	///
			{
			      call program immediate_error_1
			      exit 198
			}
			
			put main.lb_type_immediate " "
			put " " imSci1
			put " " imSci2
			put " " imSci3
			put " " imSci4
			put " " imSci5
			put " " imSci6
			put " " imSci7
			put " " imSci8
			put " " imSci9
			put " " imSci10
			put " " imSci11
			put " " imSci12
			put " " imSci13
			put " " imSci14
			put " " imSci15
			put " " imSci16
			put " " imSci17
			put " " imSci18
			put " " imSci19
			put " " imSci20
			put " " imSci21
			put " " imSci22
			put " " imSci23
			put " " imSci24
			put " " imSci25
