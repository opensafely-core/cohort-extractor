	}
	if main.rb_advanced {
		if ! H(main.ck_adv_y2) & main.ck_adv_y2 {
			call PARENT.callthru "build_isY2Set.settrue"
		}
		if H(main.ck_adv_y2) | ! main.ck_adv_y2 {
			call PARENT.callthru "build_isY2Set.setfalse"
		}
	}
	call program get_display_cmd
END
