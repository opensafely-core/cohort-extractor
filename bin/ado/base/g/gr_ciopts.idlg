PROGRAM gr_ciopts_plotList_sel
BEGIN
	call script gr_ciopts_hide_buttons
	if gr_ciopts.lb_plot.iseq("plot") {
		if gr_ciopts.cb_plot_type.iseq("") {
			call gr_ciopts.bu_rcap_d.show
			call gr_ciopts.cb_rcap_d.show
			call gr_ciopts.tx_rcap.show
		}
		if gr_ciopts.cb_plot_type.iseq("rarea") {
			call gr_ciopts.bu_rarea_d.show
		}
		if gr_ciopts.cb_plot_type.iseq("rbar") {
			call gr_ciopts.bu_rbar_d.show
		}
		if gr_ciopts.cb_plot_type.iseq("rspike") {
			call gr_ciopts.bu_rspike_d.show
		}
		if gr_ciopts.cb_plot_type.iseq("rcapsym") {
			call gr_ciopts.bu_rcaps_d.show
			call gr_ciopts.bu_rcapsm_d.show
		}
		if gr_ciopts.cb_plot_type.iseq("rscatter") {
			call gr_ciopts.bu_rscat_d.show
		}
		if gr_ciopts.cb_plot_type.iseq("rline") {
			call gr_ciopts.bu_rline_d.show
		}
		if gr_ciopts.cb_plot_type.iseq("rconnected") {
			call gr_ciopts.bu_rconn_d.show
			call gr_ciopts.bu_rconnm_d.show
		}
	}
	if gr_ciopts.lb_plot.iseq("plot1") {
		if gr_ciopts.cb_plot_type.iseq("") {
			call gr_ciopts.bu_rcap_1.show
			call gr_ciopts.cb_rcap_1.show
			call gr_ciopts.tx_rcap.show
		}
		if gr_ciopts.cb_plot_type.iseq("rarea") {
			call gr_ciopts.bu_rarea_1.show
		}
		if gr_ciopts.cb_plot_type.iseq("rbar") {
			call gr_ciopts.bu_rbar_1.show
		}
		if gr_ciopts.cb_plot_type.iseq("rspike") {
