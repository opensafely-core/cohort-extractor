PROGRAM gr_ciopts_set_defaults
BEGIN
	call create STRING ciopts_rlevel
	call create STRING ciopts_r_cmd
	call create STRING ciopts_r_cmd2
	call create STRING ciopts_mcmethod
	call create STRING ciopts_mcadjustall
	call create STRING ciopts_tmp

	call ciopts_r_cmd.setstring r(cmd)
	call ciopts_r_cmd2.setstring r(cmd2)
	call ciopts_rlevel.setstring r(level)

	if ((main_r_cmd.iseq("margins") & main_r_cmd2.iseq("")) |	///
		(main_r_cmd.iseq("contrast") & main_r_cmd2.iseq("margins"))) {
		call ciopts_mcmethod.setstring r(mcmethod)
		call ciopts_mcadjustall.setstring r(mcadjustall)
		if ciopts_mcadjustall.isneq("") {
			call ciopts_tmp.append class ciopts_mcmethod.value
			call ciopts_tmp.append " "
			call ciopts_tmp.append class ciopts_mcadjustall.value
			call ciopts_tmp.withvalue			///
				gr_ciopts.cb_mcompare.setvalue "@"
			call ciopts_tmp.withvalue			///
				gr_ciopts.cb_mcompare.setdefault "@"
		}
		if ciopts_mcadjustall.iseq("") & ciopts_mcmethod.isneq("") {
			call gr_ciopts.cb_mcompare.setvalue r(mcmethod)
			call gr_ciopts.cb_mcompare.setdefault r(mcmethod)
		}
	}
	if (main_r_cmd.iseq("pwcompare") & main_r_cmd2.iseq("margins")) {
		call ciopts_mcmethod.setstring r(mcmethod_vs)
		call ciopts_mcadjustall.setstring r(mcadjustall_vs)
		if ciopts_mcadjustall.isneq("") {
			call ciopts_tmp.append class ciopts_mcmethod.value
			call ciopts_tmp.append " "
			call ciopts_tmp.append class ciopts_mcadjustll.value
			call ciopts_tmp.withvalue			///
				gr_ciopts.cb_mcompare.setvalue "@"
			call ciopts_tmp.withvalue			///
				gr_ciopts.cb_mcompare.setdefault "@"
		}
		if ciopts_mcadjustall.iseq("") & ciopts_mcmethod.isneq("") {
			call gr_ciopts.cb_mcompare.setvalue r(mcmethod_vs)
			call gr_ciopts.cb_mcompare.setdefault r(mcmethod_vs)
		}
	}

	if ciopts_rlevel.isneq("") {
		call gr_ciopts.sp_level.setvalue r(level)
		call gr_ciopts.sp_level.setdefault r(level)
	}
END

