PROGRAM on_query
BEGIN
	put "mi query"
	stata
END

PROGRAM on_describe
BEGIN
	call create STRING _mi_marker
	call _mi_marker.setvalue char _dta[_mi_marker]
	if !_mi_marker {
		stopbox stop resource MI_MSGBOX_DATA_NOT_SET_AS_MULTIPLE_IMPUTATION
// Data have not been set as multiple imputation.
	}
	put "mi describe"
	if mi.ck_ex3 {
		put ", detail"
	}
	stata
END

PROGRAM on_misstable
BEGIN
	call create STRING misstable_cmd
	if _mi_marker {
		call create CHILD mi_misstable, allowsubmit
		call mi_misstable.setExitString "misstable_cmd"
