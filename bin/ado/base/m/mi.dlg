		}
	}

END

/* -------------------- Predict Tab (Submit) -------------------- */

PROGRAM psubmit
BEGIN
	if mi.btn_8 {
		if (mi.rb_pred) {
			put "mi predict "
		}
		if (mi.rb_prednl) {
			put "mi predictnl "
		}
		put /program pmain_output
		beginoptions
			put " " /program popt_output
			put " " /program pmiopt_output
			put " " /program prpt_output
			put " " /program padv_output
		endoptions
		stata
	}
END

