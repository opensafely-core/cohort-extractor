			"resource MI_MI_TX_EUSING" 
		stopbox stop "class _MSG_TMP.value"
// On the Main tab, "File containing saved fitted models:" must be filled in.
	}
	put " using "
	put /hidden `"""' mi.br_pmain_using `"""'
END

