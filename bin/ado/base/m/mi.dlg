PROGRAM estimates_show_tab
BEGIN
	if mi.btn_e1 & mi.btn_e1.isvisible() {
		call program OnBTN_e1
	}
	if mi.btn_e2 & mi.btn_e2.isvisible() {
		call script OnBTN_e2
	}
	if mi.btn_e3 & mi.btn_e3.isvisible() {
