			put `"(""'
			put mi.cb_eform
			put `"")"'
		}
	}
	if mi.ck_nogroup & (mi.lb_ecmd.iseq("xtreg") |			///
			mi.lb_ecmd.iseq("xtrc") |			///
			mi.lb_ecmd.iseq("xtlogit") |			///
			mi.lb_ecmd.iseq("xtprobit") |			///
			mi.lb_ecmd.iseq("xtcloglog") |			///
			mi.lb_ecmd.iseq("xtpoisson") |			///
			mi.lb_ecmd.iseq("xtnbreg") |			///
			mi.lb_ecmd.iseq("xtgee") |			///
			mi.lb_ecmd.iseq("mixed") |			///
			mi.lb_ecmd.iseq("melogit") |			///
			mi.lb_ecmd.iseq("mepoisson")) {
		put " nogroup"
	}
	if (bu_xtmeResults & (mi.lb_ecmd.iseq("mixed") |		///
		mi.lb_ecmd.iseq("melogit") |				///
		mi.lb_ecmd.iseq("mepoisson") | mi.rb_esyntax2)) {
		put " " bu_xtmeResults
	}
END

