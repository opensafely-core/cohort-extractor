PROGRAM tsubmit
BEGIN
	if mi.btn_7 {
		if mi.rb_test {
			put "mi test"
			put " " /program mi_lb_testBuild_output
			beginoptions
				if mi.ck_testopt_ufmi {
					put " ufmitest"
