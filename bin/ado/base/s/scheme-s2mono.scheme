color fpesinside_symbol gs12 
color fpesinside_line gs12 

linepattern p1lineplot  solid
linepattern p2lineplot  dash
linepattern p3lineplot  vshortdash
linepattern p4lineplot  longdash_dot
linepattern p5lineplot  longdash
linepattern p6lineplot  dash_dot
linepattern p7lineplot  dot
linepattern p8lineplot  shortdash_dot
linepattern p9lineplot  tight_dot
linepattern p10lineplot dash_dot_dot
linepattern p11lineplot longdash_shortdash
linepattern p12lineplot dash_3dot
linepattern p13lineplot longdash_dot_dot
linepattern p14lineplot shortdash_dot_dot
linepattern p15lineplot longdash_3dot
linepattern cvcvline    dash
linepattern cvlsline    solid
linepattern cvseline    vshortdash


symbol p1     circle
symbol p2     diamond
symbol p3     square
symbol p4     triangle
symbol p5     x
symbol p6     plus
symbol p7     circle_hollow
symbol p8     diamond_hollow
symbol p9     square_hollow
symbol p10    triangle_hollow
symbol p11    smcircle
symbol p12    smdiamond
symbol p13    smsquare
symbol p14    smtriangle
