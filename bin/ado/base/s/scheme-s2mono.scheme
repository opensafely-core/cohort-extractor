*                                    s2mono.scheme

* s2 scheme family with a naturally white background (white plotregions and
* lightly gray background) and gray scale foreground (lines, symbols, 
* text, etc).

*!  version 1.3.1  10sep2019

sequence 1220

#include s2color
label "s2 monochrome"

color             black
color background  gs15
color foreground  black

color symbol      black
color backsymbol  gs8

color text          black
color body          black
color heading       black
color subheading    black
color axis_title    black
color matrix_label  black
color label         black
color key_label     black
color tick_label    black
color tick_biglabel black

color box           gs13
color textbox       gs13
color mat_label_box gs13

color filled_text  black
color filled       gs13

color grid         dimgray
color major_grid   dimgray
color minor_grid   gs5

color ci_line      gs8
color ci_arealine  gs12
color ci_area      gs12
color ci_symbol    gs9
color ci2_line     gs4
color ci2_arealine gs10
color ci2_area     gs10
color ci2_symbol   gs6

color matrix         gs6    
color matrixmarkline gs6    
color refmarker      gs11
color refmarkline    black
color histogram      gs8
color histback       gs10
color histogram_line black

color xyline       gs3
color refline      gs3
color cvcvline       gs7
color cvlsline       gs1
color cvseline       gs3
color dots         black

color plotregion         white
color plotregion_line    white
color matrix_plotregion  white
color matplotregion_line black
color legend             white
color legend_line        black
color clegend            white
color clegend_outer      none
color clegend_inner      none
color clegend_line       black

color contour_begin      gs15
color contour_end        black
color zyx2               black

color p       black
color p1      gs6
color p2      gs10
color p3      gs8
color p4      gs4
color p5      black
color p6      gs12
color p7      gs2
color p8      gs7
color p9      gs9
color p10     gs11
color p11     gs13
color p12     gs5
color p13     gs3
color p14     gs12
color p15     gs5

color pline black

color plabel       black

color plineplot    black

color pmarkback      white
color pmarkbkfill    white

color sunflower      black
color sunflowerlb    gs11
color sunflowerlf    black
color sunflowerdb    gs7
color sunflowerdf    black

color fp             gs6
color fp_line        gs6
color fp_symbol      gs6 
color fpgrp          gs10
color fpgrp_arealine gs10
color fpgrp_area     gs10
color fpgrp_line     gs10
color fpgrp_symbol   gs10
color fpall          gs8
color fpall_arealine gs8
