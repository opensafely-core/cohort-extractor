// PROGRAMs called by -PROGRAM command- *************************************

PROGRAM se_output
BEGIN
	if ! se_vcetype.equals(resource SE_LBL_DFLT_STD_ERRORS) ///
	 & ! se.ck_svy {
		put /program _se_vce_output
		if se_closeParenthesis {
			put ") "
		}
		option se.ck_nonest
		option se.ck_dfadj
	}
	if se_isGLM {
		optionarg se.en_vfact
		optionarg se.en_disp
		option se.rb_deflt
		option se.rb_x2
		option se.rb_dev
		option se.rb_value
		if se.rb_value {
			require se.en_value
			optionarg se.en_value
		}
	}
	if se_isGEE {
		option se.rb_nmp
		option se.rb_rgf
		option se.rb_deflt
		option se.rb_x2
		option se.rb_dev
		option se.rb_phi
		option se.rb_value
		if se.rb_value {
			require se.en_value
			optionarg se.en_value
		}
	}
	if se_hasNoAdjust & !se.ck_noadjust & !H(se.ck_noadjust) {
		put " " "noadjust"
	}
	if se_isGMM {
		option se.ck_gmm_quickd
	}
	if se_isNpregress {
		option se.ck_bwreplace
	}
END
PROGRAM _se_vce_output
BEGIN
	// NOTE: only -PROGRAM se_output- should call this program
	put " "
 	if se_vcetype.equals(resource LBL_ROBUST) & (se.rb_hc2 | se.rb_hc3) {
		option se.rb_hc2
		option se.rb_hc3
	}
	else {
		put se.lb_vcetype
	}
	if !H(se.cb_hac) {
		require se.cb_hac
		put " " se.cb_hac
		put " " se.en_lag
	}
	if !H(se.cb_hac2) {
		require se.cb_hac2
		put " " se.cb_hac2
		if se.rb_hacopt {
			option se.rb_hacopt
			if !se.sp_hacopt_lags.isdefault() {
				put " "
				put se.sp_hacopt_lags
			}
		}
		if se.rb_haclags {
			put " " se.sp_lags
		}
	}
// CHECK IS ALSO IN SEM_BUILDER FILES sg__gestoptions and sg__estoptions
	if se_vcetype.equals(resource LBL_CLUSTERED_ROBUST) ///
	 | se_vcetype.equals(resource SE_LBL_CLUSTER) {
		require se.vn_cluster
		put " " se.vn_cluster
	}

	if se_vcetype.equals(resource LBL_HAC) {
	   // verify time settings
	   call program _se_notes_for_tsset
	   if se_tsset.equals("") {
		call se_STMP.format 					///
			"resource LBL_MSGBOX_TIME_SETTINGS_REQUIRED_FMT" ///
			"resource LBL_HAC"				///
			"resource LBL_BU_TIME_SETTINGS" 		///
			"resource SE_TITLE_SE_ROBUST"
		stopbox note class se_STMP.value
	
// HAC requires time settings. See "Time settings" on the "SE/Robust" tab.
	   }
	}

	beginoptions
		if se_vcetype.equals(resource SE_LBL_BOOTSTRAP) ///
	 	 | se_vcetype.equals(resource SE_LBL_JACKKNIFE) {
			optionarg se.vn_cluster
		}
		option se.ck_nodots
		option se.ck_mse
		option se.ck_bca
		optionarg /hidedefault se.sp_reps
		optionarg se.ed_seed
		optionarg se.sp_dots
		if se_isGMM {
			option se.ck_indep
		}
	endoptions
END

PROGRAM se_prefix_output
BEGIN
	if se.ck_svy {
		call se_svyset.setstring char _dta[_svy_version]
		call create STRING se_title
		call se_title.setstring class .se.tabtitle

// CHECK IS ALSO IN SEM_BUILDER FILES sg__gestoptions and sg__estoptions
		if se_svyset.equals("") {
			if se_title.equals(resource SE_TITLE_SE_ROBUST) {
				call se_STMP.format				///
					"resource SE_MSGBOX_SVY_SETTINGS_REQUIRED_FMT" ///
					"resource LBL_SURVEY_SETTINGS" 	   	///
					"resource SE_TITLE_SE_ROBUST"
			stopbox stop "class se_STMP.value"
// Survey estimation commands require survey settings. See "Survey settings" on the "SE/Robust" tab.
			}
			else {
			  if se_title.equals(resource SE_TITLE_SE_CLUSTER) {
				call se_STMP.format 				///
					"resource SE_MSGBOX_SVY_SETTINGS_REQUIRED_FMT" ///
					"resource LBL_SURVEY_SETTINGS" 	   	///
					"resource SE_TITLE_SE_CLUSTER"
			  stopbox stop class se_STMP.value
// Survey estimation commands require survey settings. See "Survey settings" button on the "SE/Cluster" tab.
			  }
			  else {
			    if se_title.equals(resource LBL_SE) {
				call se_STMP.format 			   	///
					"resource SE_MSGBOX_SVY_SETTINGS_REQUIRED_FMT" ///
					"resource LBL_SURVEY_SETTINGS" 	   	///
					"resource LBL_SE"
			        stopbox stop class se_STMP.value
// Survey estimation commands require survey settings. See "Survey settings" button on the "SE" tab.
			    }
			    else {
				// we really should never get here
				call se_STMP.format 			   	///
					"resource SE_MSGBOX_SVY_SETTINGS_REQUIRED_FMT" ///
					"resource LBL_SURVEY_SETTINGS" 	   	///
					"resource LBL_SE"
			        stopbox stop class se_STMP.value
// Survey estimation commands require survey settings. See "Survey settings" button on the "SE" tab.
			    }
			  }
			}
		}
		put /program _se_svy_output
		put " :"
	}
END
PROGRAM _se_svy_output
BEGIN
	// NOTE: only -PROGRAM se_prefix_output- should call this program
	put "svy"
	if se_svyvcetype.equals(resource SE_LBL_LINEARIZED) {
		put " linearized"
	}
	if se_svyvcetype.equals(resource SE_LBL_BOOTSTRAP) {
		put " bootstrap"
	}
	if se_svyvcetype.equals(resource SE_LBL_BRR) {
		put " brr"
	}
	if se_svyvcetype.equals(resource SE_LBL_JACKKNIFE) {
		put " jackknife"
	}
	if se_svyvcetype.equals(resource SE_LBL_SDR) {
		put " sdr"
	}
	beginoptions
		put /program subpop_output
		optionarg se.en_dof
		option se.ck_mse
		option se.ck_nomse
		option se.ck_nodots
		optionarg se.ed_bsn
		optionarg se.ed_hadamard
		optionarg se.ed_fay
	endoptions
END

