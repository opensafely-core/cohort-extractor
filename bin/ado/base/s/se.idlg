PROGRAM _se_check_notes
BEGIN
	call script _se_notes_hide
	call program _se_set_notes
	if se_hasNotes 							  ///
	 & se_notesVisible 						  ///
	 & ( se_vcetype.equals(resource SE_LBL_DFLT_STD_ERRORS)		  ///
	   | se_vcetype.equals(resource SE_LBL_ANALYTIC) 		  ///
	   | se_vcetype.equals(resource SE_LBL_NEWEYS_2STEP)		  ///
	   | se_vcetype.equals(resource SE_LBL_HECKMANS_2STEP)		  ///
	   | se_vcetype.equals(resource LBL_2STEP)			  ///
	   | se_vcetype.equals(resource SE_LBL_2STAGE)			  ///
	   | se_vcetype.equals(resource SE_LBL_OLS)			  ///
	   | se_vcetype.equals(resource SE_LBL_GNR)			  ///
	   | se_vcetype.equals(resource SE_LBL_GLS) 	  		  ///
	   | se_vcetype.equals(resource SE_LBL_CONVENTIONAL)		  ///
	   | se_vcetype.equals(resource LBL_GENERALIZED_METHOD_OF_MOMENTS_GMM)			  ///
	   | se_vcetype.equals(resource SE_LBL_LINEARIZED)		  ///
	   | se_vcetype.equals(resource SE_LBL_NONE)			  ///
	   | (se_default_vcetype.equals(resource LBL_ROBUST)		  ///
	   	& se_vcetype.equals(resource LBL_UNADJUSTED)		  ///
	   	& !se_isGMM & !se_isIVPOISSON &				  ///
		(!se_hasNoAdjust|se.lb_vcetype.iseq("")))		  ///
	   | se.ck_svy 							  ///
	   | (se_hasIOIM 						  ///
	   	& se_vcetype.equals(resource SE_LBL_OIM))		  ///
	   | (se_default_vcetype.equals(resource LBL_ROBUST)		  ///
	   	& se_vcetype.equals(resource LBL_ROBUST)		  ///
	   	& !se_isGMM & (!se_hasNoAdjust|se.lb_vcetype.iseq("")))   ///
	   ) 								  ///
	{
		call script _se_notes_show
	}
END
