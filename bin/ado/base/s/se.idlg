			"resource WEIGHTS_RB_PW"
 		stopbox note class se_STMP.value
 // On the "SE/Robust" tab, "OIM" standard errors have been deselected because they are not allowed with "Sampling weights".
	}
	if se_vcetype.equals(resource SE_LBL_OPG) {
		call se_vcetype.setstring resource SE_LBL_DFLT_STD_ERRORS
		call se_STMP.format 					   ///
			"resource SE_MSGBOX_DESELECTED_STDERR_WEIGHT_TYPE_FMT" ///
			"resource SE_TITLE_SE_ROBUST" 		   	   ///
			"resource SE_LBL_OPG"	 		   	   ///
			"resource WEIGHTS_RB_PW"
		stopbox note class se_STMP.value
// On the "SE/Robust" tab, "OPG" standard errors have been deselected because they are not allowed with "Sampling weights".
	}
	if se_vcetype.equals(resource SE_LBL_GLS) {
		call se_vcetype.setstring resource SE_LBL_DFLT_STD_ERRORS
		call se_STMP.format 					   ///
			"resource SE_MSGBOX_DESELECTED_STDERR_WEIGHT_TYPE_FMT" ///
			"resource SE_TITLE_SE_ROBUST" 		   	   ///
			"resource SE_LBL_GLS"	 		   	   ///
			"resource WEIGHTS_RB_PW"
		stopbox note class se_STMP.value
// On the "SE/Robust" tab, "GLS" standard errors have been deselected because they are not allowed with "Sampling weights".
	}
	if se_vcetype.equals(resource SE_LBL_EIM) {
		call se_vcetype.setstring resource SE_LBL_DFLT_STD_ERRORS
		call se_STMP.format 					   ///
			"resource SE_MSGBOX_DESELECTED_STDERR_WEIGHT_TYPE_FMT" ///
			"resource SE_TITLE_SE_ROBUST" 		   	   ///
			"resource SE_LBL_EIM"	 		   	   ///
			"resource WEIGHTS_RB_PW"
		stopbox note class se_STMP.value
// On the "SE/Robust" tab, "EIM" standard errors have been deselected because they are not allowed with "Sampling weights".
	}
