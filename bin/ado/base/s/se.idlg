		call se_svy_change.Arrpush "program _se_svy_bootstrap"
	
		call se_svy_contents.ArrpushR resource dlg SE_LBL_BRR
		call se_svy_values.Arrpush "brr"
		call se_svy_change.Arrpush "program _se_svy_brr"
	
