PROGRAM _se_hideOrShow
BEGIN
	call se.tx_temp.hide
	call program _se_check_bootstrap
	call program _se_check_cluster
	call program _se_check_factors
	call program _se_check_scale
	call program _se_check_hac
	call program _se_check_mse
	call program _se_check_noadjust
	call program _se_check_nodots
	call program _se_check_notes
	call program _se_notes_for_tsset
	call program _se_check_vfactor
	call program _se_check_svy
	call program _se_check_bias
	if se.ck_svy {
		call script _se_switchToSvy
	}
	if se_hasSvy & ! se.ck_svy {
		call script _se_switchToNonSvy
	}
	call program _se_check_gmm
END

SCRIPT _se_check_gmm
BEGIN
	if se_isGMM {
		if (se_vcetype.equals(resource SE_LBL_DFLT_STD_ERRORS)) {
			call se.ck_indep.setposition . 170 
			call se.ck_indep.show
			call se.ck_gmm_quickd.setposition . 210
		}
		if (se_vcetype.equals(resource LBL_ROBUST)) {
			call se.ck_indep.setposition . 140
			call se.ck_indep.show
			call se.ck_gmm_quickd.setposition . 165
		}
		if (se_vcetype.equals(resource SE_LBL_BOOTSTRAP)) {
			call se.ck_indep.hide
			call se.ck_gmm_quickd.setposition . 280
		}
		if (se_vcetype.equals(resource LBL_CLUSTERED_ROBUST)) {
			call se.ck_indep.setposition . 190 
			call se.ck_indep.show
			call se.ck_gmm_quickd.setposition . 215
		}
		if (se_vcetype.equals(resource SE_LBL_CLUSTER)) {
			call se.ck_indep.setposition . 170
			call se.ck_indep.show
			call se.ck_gmm_quickd.setposition . 195
		}
		if (se_vcetype.equals(resource SE_LBL_JACKKNIFE)) {
			call se.ck_indep.hide
			call se.ck_gmm_quickd.setposition . 240
		}
		if (se_vcetype.equals(resource LBL_HAC)) {
			call se.ck_indep.setposition . 310
			call se.ck_indep.show
			call se.ck_gmm_quickd.setposition . 335
		}
		if (se_vcetype.equals(resource LBL_UNADJUSTED)) {
			call se.ck_indep.setposition . 140
			call se.ck_indep.show
			call se.ck_gmm_quickd.setposition . 165
		}
		call se.ck_gmm_quickd.show
	}
	else {
		call se.ck_indep.hide
		call se.ck_gmm_quickd.hide
	}
END

