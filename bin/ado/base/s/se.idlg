	   (se_vcetype.equals(resource SE_LBL_BOOTSTRAP) 		///
	   	| se_vcetype.equals(resource SE_LBL_JACKKNIFE)) {
		if se_noCluster {
			call se.ck_mse.setposition . 165
		}
		if ! se_noCluster {
			call se.ck_mse.setposition . 215
		}
		call script _se_mse_show
	}
	if se.ck_svy	///
	 & (se_svyvcetype.equals(resource SE_LBL_BOOTSTRAP) 		///
	 |  se_svyvcetype.equals(resource SE_LBL_BRR) 			///
	 |  se_svyvcetype.equals(resource SE_LBL_SDR) 			///
	 |  se_svyvcetype.equals(resource SE_LBL_JACKKNIFE)) {
		call se.ck_nomse.setposition . 170
		call se.ck_mse.setposition . 170
		call se_svymse.setstring char _dta[_svy_mse]
		if se_svymse.equals("mse") {
			call script _se_nomse_show
		}
		if ! se_svymse.equals("mse") {
			call script _se_mse_show
		}
	}
END
