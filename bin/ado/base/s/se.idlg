PROGRAM _se_check_cluster
BEGIN
	call script _se_cluster_hide
	call se.ck_nonest.hide
	call se.ck_dfadj.hide
	if !se.ck_svy & se_hasCluster & !se_noCluster & 	///
	 ( se_vcetype.equals(resource SE_LBL_BOOTSTRAP) 	///
	 | se_vcetype.equals(resource LBL_CLUSTERED_ROBUST)	///
	 | se_vcetype.equals(resource SE_LBL_CLUSTER) 		///
	 | se_vcetype.equals(resource SE_LBL_JACKKNIFE) 	///
	 ) {
		if se_vcetype.equals(resource LBL_CLUSTERED_ROBUST) 	///
		 | se_vcetype.equals(resource SE_LBL_CLUSTER) 		{
			call se.tx_cluster.setlabel resource LBL_CLUSTER_VARIABLE
			if se_hasClusterDFADJ {
				call se.ck_dfadj.show
			}
			if ! se_hasClusterDFADJ {
				call se.ck_dfadj.hide
			}
		}
		if !se_vcetype.equals(resource LBL_CLUSTERED_ROBUST)	///
		 & !se_vcetype.equals(resource SE_LBL_CLUSTER) {
			call se.tx_cluster.setlabel ///
				resource SE_TX_CLUSTER_OPTIONAL
		}
		call script _se_cluster_show
		if se_hasClusterNONEST {
			if se_vcetype.equals(resource LBL_CLUSTERED_ROBUST) ///
			 | se_vcetype.equals(resource SE_LBL_CLUSTER)  {
				call se.ck_nonest.setposition  . 190
			}
			if se_vcetype.equals(resource SE_LBL_BOOTSTRAP) {
				call se.ck_nonest.setposition  . 265
			}
			if se_vcetype.equals(resource SE_LBL_JACKKNIFE) {
				call se.ck_nonest.setposition  . 240
			}
			call se.ck_nonest.show
		}
		if ! se_hasClusterNONEST {
			call se.ck_nonest.hide
		}
	}
END
