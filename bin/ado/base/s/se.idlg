	 ) {
		call se.rb_rgf.show
		call se.rb_rgf.setlabel "resource SE_RB_RGF"
		call se.gb_factor.setposition . . . 90
	}
	if !se_hasRGF | 					///
	 ( !se_vcetype.equals(resource LBL_ROBUST) 		///
	 & !se_vcetype.equals(resource LBL_CLUSTERED_ROBUST) ///
	 ) {
		call se.rb_rgf.hide
		if se.rb_rgf {
			call se.rb_default.seton
		}
		call se.gb_factor.setposition . . . 70
