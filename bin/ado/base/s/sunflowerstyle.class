// sunflowerstyle
//
// Base and default class for sunflower style
//
*! version 1.0.0  22may2003
version 8

class {

    instance:

	// light sunflower styles
	lighthexbin	= .areastyle.new
	lightflower	= .linestyle.new

	// dark sunflower styles
	darkhexbin	= .areastyle.new
	darkflower	= .linestyle.new

}, inherit(seriesstyle)

//  Don't bother with a new, this will always be -.setstyle-d 

//  Don't bother with a setgdi, the marker and hexbins will conflict, holder
//  should invoke .setgdi's for the marker, *hexbin, and *flower as needed.

exit
