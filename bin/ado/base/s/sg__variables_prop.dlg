SCRIPT flink_PREINIT
BEGIN
	create STRING sFlinkStyle
	create STRING sFlinkOrder

	program flink_get_defaults
END

SCRIPT flink_POSTINIT
BEGIN
	program flink_initialize_controls
END

PROGRAM flink_get_defaults
BEGIN
	call sSrcType.withvalue sFlinkStyle.setvalue class .__SEMG.dbsettings.@_famlinklook
	call sFlinkStyle.withvalue flink.cb_ostyle.setdefault "@"

	call sSrcType.withvalue sFlinkOrder.setvalue class .__SEMG.dbsettings.@_famlinkplace
	call sFlinkOrder.withvalue flink.cb_order.setdefault "@"
END

PROGRAM flink_initialize_controls
BEGIN
	call flink.cb_ostyle.setvalue class sFlinkStyle.value
	call flink.cb_order.setvalue class sFlinkOrder.value

	if bGLM {
		call flink.gb_flink.enable
		call flink.tx_ostyle.enable
		call flink.cb_ostyle.enable
		call flink.tx_order.enable
		call flink.cb_order.enable
		call flink.tx_appear1.enable
		call flink.bu_appear1.enable
	}
	else {
		call flink.gb_flink.disable
		call flink.tx_ostyle.disable
		call flink.cb_ostyle.disable
		call flink.tx_order.disable
		call flink.cb_order.disable
		call flink.tx_appear1.disable
		call flink.bu_appear1.disable
	}

	call program ostyle_changed
END

PROGRAM show_appearance_dlg1
BEGIN
	call create STRING sDlg1
	call OBJ.withvalue sDlg1.setvalue "@"
	call sDlg1.append " "
	call sDlg1.append class flink.cb_ostyle.value
	call sDlg1.withvalue create CHILD sg__variables_prop_flink, message("@")

	if (flink.cb_ostyle.iseqignorecase("fancybox")) {
		call sg__variables_prop_flink.settitle resource LBL_TITLE_APPEARANCE_SETTINGS_FANCY_BOX
	}
	if (flink.cb_ostyle.iseqignorecase("famlink")) {
		call sg__variables_prop_flink.settitle resource LBL_TITLE_APPEARANCE_SETTINGS_FAM_LINK
	}
	if (flink.cb_ostyle.iseqignorecase("family")) {
		call sg__variables_prop_flink.settitle resource LBL_TITLE_APPEARANCE_SETTINGS_FAM
	}
	if (flink.cb_ostyle.iseqignorecase("link")) {
		call sg__variables_prop_flink.settitle resource LBL_TITLE_APPEARANCE_SETTINGS_LINK
	}
END

PROGRAM ostyle_changed
BEGIN
	if !flink.cb_ostyle.isenabled() {
		exit
	}

	if (flink.cb_ostyle.iseqignorecase("fancybox")) {
		call flink.tx_order.disable
		call flink.cb_order.disable
		call flink.bu_appear1.enable
		call flink.bu_appear1.setlabel resource SG__VARIABLES_PROP_FANCY_BOX
	}
	if (flink.cb_ostyle.iseqignorecase("std")) {
		call flink.tx_order.disable
		call flink.cb_order.disable
		call flink.bu_appear1.disable
		call flink.bu_appear1.setlabel resource SG__VARIABLES_PROP_SETTINGS
	}
	if (flink.cb_ostyle.iseqignorecase("famlink")) {
		call flink.tx_order.enable
		call flink.cb_order.enable
		call flink.bu_appear1.enable
		call flink.bu_appear1.setlabel resource SG__VARIABLES_PROP_FAMILY_AND_LINK
	}
	if (flink.cb_ostyle.iseqignorecase("family")) {
		call flink.tx_order.enable
		call flink.cb_order.enable
		call flink.bu_appear1.enable
		call flink.bu_appear1.setlabel resource SG__VARIABLES_PROP_FAMILY_ONLY
	}
	if (flink.cb_ostyle.iseqignorecase("link")) {
		call flink.tx_order.enable
		call flink.cb_order.enable
		call flink.bu_appear1.enable
		call flink.bu_appear1.setlabel resource SG__VARIABLES_PROP_LINK_ONLY
	}
END

DIALOG flink, tabtitle(resource SG__VARIABLES_PROP_TAB_FLINK)
BEGIN
  GROUPBOX gb_flink	_lft	_top	_iwd	_ht11,		///
	label(resource SG__VARIABLES_PROP_FLINK_GB_FLINK)

  TEXT     tx_ostyle	_ilft	_ms	_wd	.,		///
	label(resource SG__VARIABLES_PROP_FLINK_TX_OSTYLE) right
  COMBOBOX cb_ostyle	_sep	@	160	., nomemory	///
	dropdownlist contents(.__SEM_STYLES_LABEL.famlinklook)	///
	values(.__SEM_STYLES.famlinklook)			///
	onselchange(program ostyle_changed)

  TEXT     tx_order	_ilft	_mls	_wd	.,		///
  	label(resource SG__VARIABLES_PROP_FLINK_TX_ORDER) right
  COMBOBOX cb_order	_sep	@	160	., nomemory	///
	dropdownlist contents(__SEM_STYLES_LABEL.famlinkplace)	///
	values(__SEM_STYLES.famlinkplace)

  TEXT     tx_appear1	_ilft	+35	_ibwd	.,		///
	label(resource SG__VARIABLES_PROP_FLINK_TX_APPEAR1)
  BUTTON    bu_appear1	55	_ss	130	.,		///
	onpush(program show_appearance_dlg1)
END

PROGRAM flink_output
BEGIN
	call program flink_get_defaults

	call create STRING sFLinkTmp
	if (sSrcType.iseq("nodestyle")) {
		call sFLinkTmp.setvalue "SetStyleSelected"
	}
	else { 
		call sFLinkTmp.setvalue "SetStyle"
	}

	if (!flink.cb_ostyle.isdefault()) {
		call sg__PARAM2.setvalue class sFLinkTmp.value
		call sg__ARG_00.setvalue class sSrcType.value
		call sg__ARG_00.append ".famlinklook"
		call sg__ARG_01.setvalue class flink.cb_ostyle.value
		put /program sg__do_output_sub
	}

	if (flink.cb_order.isenabled() & !flink.cb_order.isdefault()) {
		call sg__PARAM2.setvalue class sFLinkTmp.value
		call sg__ARG_00.setvalue class sSrcType.value
		call sg__ARG_00.append ".famlinkplace"
		call sg__ARG_01.setvalue class flink.cb_order.value
		put /program sg__do_output_sub
	}
