PROGRAM result_initialize_controls
BEGIN
	call create BOOLEAN bResExog
	call create BOOLEAN bResEndog
	call create BOOLEAN bResErr

	call bResExog.setfalse 
	call bResEndog.setfalse
	call bResErr.setfalse 

