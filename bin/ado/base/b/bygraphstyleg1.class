*! version 1.0.1  13jan2003
version 8

class {
	all_labels	= .yesno.new
	all_ticks	= .yesno.new
	separate_total	= .yesno.new

	plotregionstyle	= .plotregionstyle.new
} , inherit(graphstyle)


program new
